#include <stdio.h>

int main(){
    int num;

    num = 10;
    printf("Valor atual = %d \n", num);

    num += 5;
    printf("somando 5 = %d \n", num);

    num -=1;
    printf("subtraindo 1 = %d \n", num);

    num *=2;
    printf("Multiplicado por 2 = %d \n", num);

    num /=7;
    printf("Dividido por 7 = %d \n", num);

    return 0;
}
