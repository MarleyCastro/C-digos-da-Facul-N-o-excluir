#include <stdio.h>
#include <stdlib.h>

int main() {
	
	int total, num1, num2;
	
	printf("Digite o valor 1:");
	
	scanf("%d", &num1);
	
	printf("Digite o valor 2:");
	
	scanf("%d", &num2);
	
	total = num1+num2;
	
	printf("o valor da soma dos dois valores digitados e: %d\n", total);
	
	
	
	return 0;
}
