#include <stdio.h>
#include <string.h>

int main(){
    char nome_cliente[50] = "Fulano de tal";
    int i;
    int cont = 0;

    for ( i = 0; nome_cliente[i] != '\0' ; i++)
    {
        printf("Valor elemento %d da string = %c \n", i, nome_cliente[i]);
        cont++;
    }
    printf("Quantidade de caracteres da string: %d \n", cont);
    printf("Quantidade de caracteres usando sterlen: %d \n", strlen(nome_cliente));

    return 0;
}
