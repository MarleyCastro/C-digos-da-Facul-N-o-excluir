#include <stdio.h>

int main(){
    int n1, n2;

    printf("Digite o primeiro numero: ");
    scanf("%d", &n1);

    printf("Digite o segundo numero: ");
    scanf("%d", &n2);

    if (n1 == n2)
    {
        printf(" o numero sao iguais");
    }else if (n1 > n2)
    {
        printf("o maior valor e = %d", n1);
    }else{
        printf("o maior valor r = %d\n ", n2);
    }
    
    return 0;
    
}
