#include <stdio.h>

int main()
{
    int n1, n2;
    printf("Digite o primeiro numero: ");
    scanf("%d", &n1);

    printf("digite o segundo numeto: ");
    scanf("%d", &n2);

    if (n1 == n2)
    {
        printf("os numeros sao iguais! \n");
        printf("por favor digite um numero diferentes! \n");
    }
    else if (n1 > n2)
        printf("o maior valor e = %d", n1);
    else
        printf("o maior valor e = %d\n", n2);

    return 0;
}
