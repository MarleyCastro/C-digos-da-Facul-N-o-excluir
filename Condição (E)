#include <stdio.h>
//condição e(^)
int main(){
    float notaf, freq;

    printf("Digite a primeira nota: ");
    scanf("%f", &notaf);

    printf("frequencia: ");
    scanf("%f", &freq);

    //      V       e       V
    if (notaf >= 6 && freq >= 75)
    {
        printf("Aprovado");
    }else{
        printf("reprovado");
    }
    return 0;
}
